    "16396925341db07e0b": {
      "type": "dynamic-rich-text",
      "settings": {
        "width": "full-width",
        "title": "",
        "text": "<p><em>‡Points earned in Love Wellness Rewards program can be applied to one time and subscription orders in increments of $5, up to a $20 discount per order. One reward discount per order, cannot be stacked with other codes\/offers. Points are subject to restrictions, as is use and participation in Rewards Program. See FAQ and Terms of Service for details.<br\/><br\/>‡$10 off first order of $25 or more when purchasing through referral link. Referral offer valid for new customers only. Referral reward delivered as points for every eligible referral. Restrictions apply, see FAQ for details.<\/em><\/p>",
        "heading_size": "medium",
        "text_alignment": "left"
      }
    }
